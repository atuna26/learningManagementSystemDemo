<div class="content-body" style="min-height: 862px;">
    <div class="container-fluid">
        <div class="row page-titles mx-0">
            <div class="col-sm-2 p-md-0">
                <div class="welcome-text">
                    <h4>Report</h4>
                    <p class="mb-0">Form Report</p>
                </div>
            </div>
        </div>
        <!-- row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">Course List</h4>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-responsive-md">
                                <thead>
                                    <tr>
                                        <th><strong>Form Name</strong></th>
                                        <th><strong>Form For</strong></th>
                                        <th><strong>Form Kind</strong></th>
                                        <th><strong>Action</strong></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#each form}}
                                    {{#if (eq formType "Schema")}}
                                    <tr>
                                        <td>{{formName}}</td>
                                        <td>{{formFor}}</td>
                                        <td>{{formKind}} </td>
                                        <td><a class="badge light badge-success" href="/report/form-reports/{{_id}}">View</a></td>
                                    </tr>
                                    {{/if}}
                                    {{/each}}
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>