<!DOCTYPE html>
<html lang="en" class="h-100">

<head>


    <!-- Mobile Specific -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- PAGE TITLE HERE -->
    <title>PB Education Online Learning System</title>

    {{#if gridjs}}
    <!-- gridjs css -->
    <link rel="stylesheet" href="/assets/libs/gridjs/theme/mermaid.min.css">
    {{/if}}

    <!-- plugin css -->
    <link href="/assets/libs/jsvectormap/css/jsvectormap.min.css" rel="stylesheet" type="text/css" />

    <!-- Layout config Js -->
    <script src="/assets/js/layout.js"></script>
    <!-- Bootstrap Css -->
    <link href="/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <!-- Icons Css -->
    <link href="/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <!-- App Css-->
    <link href="/assets/css/app.min.css" rel="stylesheet" type="text/css" />
</head>
<style>
    @media print {
        .btn {
            display: none;
        }
    }
</style>

<body class="body  h-100">
    <div class="authincation h-100">
        <div class="container h-100">
            <div class="row justify-content-center h-100 align-items-center">
                <div class="media d-flex align-items-center justify-content-end my-2">
                </div>
                <div class="col-md-12">
                    <div class="card-header d-flex justify-content-between">
                        <h4 class="card-title text-end">feedback ({{startDate}}-{{endDate}}) for {{userName}} </h4>
                        <img src="/lms/pblogo.webp" width="100" alt="">
                    </div>
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-header text-center d-flex justify-content-center">
                                <h4 class="card-title text-center">Weekly Lecture List => General Information</h4>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-bordered verticle-middle table-responsive-sm">
                                        <thead>
                                            <tr>
                                                <th scope="col"><strong>ID</strong></th>
                                                <th scope="col"><strong>Date</strong></th>
                                                <th scope="col"><strong>Course</strong></th>
                                                <th scope="col"><strong>Tutor</strong></th>
                                                <th scope="col"><strong>Topic</strong></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each formNew}}
                                            <tr>
                                                <td>{{@index}}</td>
                                                <td>{{questAndAnswer.4.answer}} / {{questAndAnswer.3.answer}}</td>
                                                <td>{{questAndAnswer.6.answer.lessonName}}</td>
                                                <td>{{questAndAnswer.1.answer.userName}}
                                                    {{questAndAnswer.1.answer.userSurname}}</td>
                                                <td>{{questAndAnswer.8.answer}}</td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-header text-center d-flex justify-content-center">
                                <h4 class="card-title text-center">Weekly Lecture List => Performance</h4>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-bordered verticle-middle table-responsive-sm">
                                        <thead>
                                            <tr>
                                                <th scope="col"><strong>ID</strong></th>
                                                <th scope="col"><strong>Date</strong></th>
                                                <th scope="col"><strong>Type</strong></th>
                                                <th scope="col"><strong>Attandance</strong></th>
                                                <th scope="col"><strong>Participation Level</strong></th>
                                                <th scope="col"><strong>Lesson Comprehension</strong></th>
                                                <th scope="col"><strong>Productivity</strong></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each formNew}}
                                            <tr class="text-center">
                                                <td>{{@index}} </td>
                                                <td>{{questAndAnswer.4.answer}} / {{questAndAnswer.3.answer}}</td>
                                                <td>{{#if (eq questAndAnswer.5.answer "Office")}}<i
                                                        class="fa-solid fa-school"></i> {{else if (eq
                                                    questAndAnswer.5.answer "Online") }}<i
                                                        class="fa-solid fa-desktop"></i> {{else if (eq
                                                    questAndAnswer.5.answer "Home Schooling") }}<i
                                                        class="fa-solid fa-house-user"></i>{{/if}}</td>
                                                <td>{{questAndAnswer.11.answer}}</td>
                                                <td>{{questAndAnswer.12.answer}}</td>
                                                <td>{{questAndAnswer.13.answer}}</td>
                                                <td>{{questAndAnswer.14.answer}}</td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">General Impressions & Suggestion/Concern for Future Lessons:</h4>
                            </div>
                            <div class="card-body">
                                <div class="table">
                                    <table class="table align-middle">
                                        <tbody>
                                            {{#each formNew}}
                                            <tr>
                                                <td><strong>{{questAndAnswer.6.answer.lessonName}}
                                                        ({{questAndAnswer.4.answer}} /
                                                        {{questAndAnswer.3.answer}})</strong></td>
                                                <td style="word-wrap: break-word;white-space: normal;"><span
                                                        class="text-success">General Impressions=></span>
                                                    {{questAndAnswer.15.answer}} <br>
                                                    <span class="text-success">Concern for Future Lessons=></span>
                                                    {{questAndAnswer.16.answer}}
                                                </td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <button type="button" class="btn btn-danger light" onclick="print()">Print</button>

                </div>
            </div>
        </div>
    </div>
    <!--**********************************
	Scripts
***********************************-->
    <!-- Required vendors -->
    <!-- JAVASCRIPT -->
    <script src="/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/assets/libs/simplebar/simplebar.min.js"></script>
    <script src="/assets/libs/node-waves/waves.min.js"></script>
    <script src="/assets/libs/feather-icons/feather.min.js"></script>
    <script src="/assets/js/plugins.js"></script>

    {{#if listjs}}
    <!-- listjs init -->
    <script src="/assets/js/pages/listjs.init.js"></script>
    {{/if}}

    {{#if prismjs}}
    <!-- prismjs plugin -->
    <script src="/assets/libs/prismjs/prism.js"></script>
    <script src="/assets/libs/list.js/list.min.js"></script>
    <script src="/assets/libs/list.pagination.js/list.pagination.min.js"></script>
    {{/if}}

    {{#if gridjs}}
    <!-- gridjs js -->
    <script src="/assets/libs/gridjs/gridjs.umd.js"></script>
    <!-- gridjs init -->
    <script src="/assets/js/pages/gridjs.init.js"></script>
    {{/if}}

    {{#if ckeditor}}
    <!-- ckeditor -->
    <script src="/assets/libs/@ckeditor/ckeditor5-build-classic/build/ckeditor.js"></script>
    {{/if}}

    {{#if apexcharts}}
    <!-- apexcharts -->
    <script src="/assets/libs/apexcharts/apexcharts.min.js"></script>
    {{/if}}

    {{#if vectorMap}}
    <!-- Vector map-->
    <script src="/assets/libs/jsvectormap/js/jsvectormap.min.js"></script>
    <script src="/assets/libs/jsvectormap/maps/world-merc.js"></script>
    {{/if}}

    {{#if dashboardInit}}
    <!-- Dashboard init -->
    <script src="/assets/js/pages/dashboard.init.js"></script>
    {{/if}}

    <!-- Sweet Alerts js -->
    <script src="/assets/libs/sweetalert2/sweetalert2.min.js"></script>
    <!-- App js -->
    <script src="/assets/js/app.js"></script>
</body>

</html>